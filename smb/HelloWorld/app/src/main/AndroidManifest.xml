<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helloworld">
    <permission-group android:name="com.example.helloworld.my_permissions"
        android:label="my permissions group"/>
    <permission android:name="com.example.helloworld.MY_PERMISSION"
        android:permissionGroup="com.example.helloworld.my_permissions"
        android:label="my permission"/>

    <uses-permission android:name="com.example.helloworld.my_permissions.MY_PERMISSION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/hello">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OptionsActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/ustawienia" />
        <activity
            android:name=".ListActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_name"/>

        <provider
            android:authorities="com.example.helloworld.provider"
            android:name="com.example.helloworld.MyProvider"
            android:exported="true"/>
    </application>

</manifest>